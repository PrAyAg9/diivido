{
  "expo": {
    "name": "Divido - Expense Tracker",
    "slug": "divido-expense-tracker",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/images/icon.png",
    "scheme": "divido",
    "userInterfaceStyle": "automatic",
    "newArchEnabled": true,
    "extra": {
      "eas": {
        "projectId": "divido-expense-tracker-2025"
      }
    },
    "ios": {
      "supportsTablet": true,
      "bundleIdentifier": "com.prayag.divido"
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/images/icon.png",
        "backgroundColor": "#10B981"
      },
      "package": "com.prayag.divido"
    },
    "web": {
      "bundler": "metro",
      "output": "single",
      "favicon": "./assets/images/favicon.png"
    },
    "plugins": [
      "expo-router",
      "expo-font",
      "expo-web-browser",
      "expo-speech-recognition"
    ],
    "experiments": {
      "typedRoutes": true
    }
  }
}
